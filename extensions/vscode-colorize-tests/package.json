{
  "name": "vscode-colorize-tests",
  "description": "Colorize tests for VS Code",
  "version": "0.0.1",
  "publisher": "vscode",
  "license": "MIT",
  "private": true,
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./out/colorizerTestMain",
  "enableProposedApi": true,
  "engines": {
    "vscode": "*"
  },
  "scripts": {
    "build": "npx webpack-cli --config extension.webpack.config --mode production",
    "watch": "npx webpack-cli --config extension.webpack.config --mode production --watch",
    "vscode:prepublish": "npm run build"
  },
  "dependencies": {
    "jsonc-parser": "2.2.1"
  },
  "devDependencies": {
    "@types/node": "^12.19.9"
  },
  "contributes": {
    "semanticTokenTypes": [
      {
        "id": "testToken",
        "description": "A test token"
      }
    ],
    "semanticTokenModifiers": [
      {
        "id": "testModifier",
        "description": "A test modifier"
      }
    ],
    "semanticTokenScopes": [
      {
        "scopes": {
          "testToken": [
            "entity.name.function.special"
          ]
        }
      }
    ],
    "productIconThemes": [
      {
        "id": "Test Product Icons",
        "label": "The Test Product Icon Theme",
        "path": "./producticons/test-product-icon-theme.json",
        "_watch": true
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/vscode.git"
  }
}
