{
  "name": "simple-browser",
  "displayName": "%displayName%",
  "description": "%description%",
  "enableProposedApi": true,
  "version": "1.0.0",
  "publisher": "vscode",
  "license": "MIT",
  "aiKey": "AIF-d9b70cd4-b9f9-4d70-929b-a071c400b217",
  "engines": {
    "vscode": "^1.53.0"
  },
  "main": "./out/extension",
  "browser": "./dist/browser/extension",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:simpleBrowser.show",
    "onCommand:simpleBrowser.api.open",
    "onOpenExternalUri:http",
    "onOpenExternalUri:https"
  ],
  "contributes": {
    "commands": [
      {
        "command": "simpleBrowser.show",
        "title": "Show",
        "category": "Simple Browser"
      }
    ],
    "configuration": [
      {
        "title": "Simple Browser",
        "properties": {
          "simpleBrowser.focusLockIndicator.enabled": {
            "type": "boolean",
            "default": true,
            "title": "Focus Lock Indicator Enabled",
            "description": "%configuration.focusLockIndicator.enabled.description%"
          }
        }
      }
    ]
  },
  "scripts": {
    "build": "npx webpack-cli --config extension.webpack.config --mode production",
    "watch": "npx webpack-cli --config extension.webpack.config --mode production --watch",
    "vscode:prepublish": "npm run build",
    "build-preview": "webpack --mode development",
    "build-preview-production": "webpack --mode production",
    "build-web": "npx webpack-cli --config extension-browser.webpack.config --mode none",
    "watch-web": "npx webpack-cli --config extension-browser.webpack.config --mode none --watch --info-verbosity verbose"
  },
  "dependencies": {
    "vscode-codicons": "^0.0.12",
    "vscode-extension-telemetry": "0.1.1",
    "vscode-nls": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.11.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/vscode.git"
  }
}
